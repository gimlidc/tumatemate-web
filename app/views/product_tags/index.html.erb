<div class="row">
	<h1>Listing product_tags</h1>

	<table>
		<tr>
			<th>Tagid</th>
			<th>Productid</th>
			<th></th>
			<th></th>
			<th></th>
		</tr>

		<% @product_tags.each do |product_tag| %>
		<tr>
			<td><%= product_tag.tagId %></td>
			<td><%= product_tag.productId %></td>
			<td><%= link_to 'Show', product_tag %></td>
			<td><%= link_to 'Edit', edit_product_tag_path(product_tag) %></td>
			<td><%= link_to 'Destroy', product_tag, :method => :delete, :confirm => 'Are you sure?' %></td>
		</tr>
		<% end %>
	</table>

	<br />

	<%= link_to 'New Product tag', new_product_tag_path %>
</div>
